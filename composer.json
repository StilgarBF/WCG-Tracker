        "influxdata/influxdb-client-php": "^3.7",
        "php-http/guzzle7-adapter": "^1.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
